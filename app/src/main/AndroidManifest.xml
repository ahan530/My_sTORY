<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="qf.com.my_story">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA"/>


    <application
        android:name=".fragment.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <!-- 首页 -->
        <activity android:name=".view.FirstActivity" />
        <!-- 引导页 -->
        <activity android:name=".view.GuideActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 登陆页 -->
        <activity android:name=".view.LogInActivity" />
        <!-- 注册页 -->
        <activity android:name=".view.RegisterActivity" />
        <activity android:name=".view.MyStoryActivity" />
        <activity android:name=".view.StoryDetailsActivity" />
        <activity android:name=".view.NewStoryActivity" />
        <activity android:name=".view.SettingActivity" />
        <activity android:name=".view.MyCenterActivity" />
        <activity android:name=".view.ChangedPwdActivity"></activity>
    </application>

</manifest>